<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dt.tarmag.model.WorkerTypePartition" table="DT_WORKER_TYPE_PARTITION">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long">
			<column name="ID" />
			<generator class="identity" />
		</id>

		<property name="workerId" type="java.lang.Long">
			<column name="WORKER_ID" />
		</property>
		<property name="partitionId" type="java.lang.Long">
			<column name="PARTITION_ID" />
		</property>
		<property name="wtypeId" type="java.lang.Long">
			<column name="WTYPE_ID" />
		</property>
		
		<property name="deleted" type="java.lang.String">
			<column name="DELETED" />
		</property>
	</class>
</hibernate-mapping>
