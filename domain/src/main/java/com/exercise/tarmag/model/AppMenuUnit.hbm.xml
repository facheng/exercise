<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dt.tarmag.model.AppMenuUnit" table="DT_APP_MENU_UNIT">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long">
			<column name="ID" />
			<generator class="identity" />
		</id>

		<property name="unitId" type="java.lang.Long">
			<column name="UNIT_ID" />
		</property>
		<property name="menuId" type="java.lang.Long">
			<column name="MENU_ID" />
		</property>
		<property name="menuName" type="java.lang.String">
			<column name="MENU_NAME" />
		</property>
		<property name="linkURL" type="java.lang.String">
			<column name="LINK_URL" />
		</property>
		<property name="isDefault" type="java.lang.Byte">
			<column name="IS_DEFAULT" />
		</property>
	</class>
</hibernate-mapping>
