<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dt.tarmag.model.PropertyChargeRule" table="DT_PROPERTY_CHARGE_RULE">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long">
			<column name="ID" />
			<generator class="identity" />
		</id>

		<property name="partitionId" type="java.lang.Long">
			<column name="PARTITION_ID" />
		</property>
		<property name="cycle" type="java.lang.Byte">
			<column name="CYCLE" />
		</property>
		<property name="ctype" type="java.lang.Byte">
			<column name="CTYPE" />
		</property>
		<property name="feeArea" type="java.lang.Double">
			<column name="FEE_AREA" />
		</property>
		<property name="feeLift" type="java.lang.Double">
			<column name="FEE_LIFT" />
		</property>
		<property name="feeAll" type="java.lang.Double">
			<column name="FEE_ALL" />
		</property>
		<property name="basementFeeArea" type="java.lang.Double">
			<column name="BASEMENT_FEE_AREA" />
		</property>
		<property name="isEffect" type="java.lang.Byte">
			<column name="IS_EFFECT" />
		</property>

		<property name="createUserId" type="java.lang.Long">
			<column name="CREATE_USER_ID" />
		</property>
		<property name="createDateTime" type="java.util.Date">
			<column name="CREATE_DATE_TIME" />
		</property>
		<property name="updateUserId" type="java.lang.Long">
			<column name="UPDATE_USER_ID" />
		</property>
		<property name="updateDateTime" type="java.util.Date">
			<column name="UPDATE_DATE_TIME" />
		</property>
		<property name="deleted" type="java.lang.String">
			<column name="DELETED" />
		</property>
	</class>
</hibernate-mapping>
