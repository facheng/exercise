<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dt.tarmag.model.District" table="DT_DISTRICT">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long">
			<column name="ID" />
			<generator class="identity" />
		</id>

		<property name="districtId" type="java.lang.Long">
			<column name="DISTRICT_ID" />
		</property>
		<property name="districtName" type="java.lang.String">
			<column name="DISTRICT_NAME" />
		</property>
		<property name="cityId" type="java.lang.Long">
			<column name="CITY_ID" />
		</property>
		<property name="locale" type="java.lang.String">
			<column name="LOCALE" />
		</property>
	</class>
</hibernate-mapping>
