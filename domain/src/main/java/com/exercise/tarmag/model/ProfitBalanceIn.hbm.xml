<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dt.tarmag.model.ProfitBalanceIn" table="DT_PROFIT_BALANCE_IN">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long">
			<column name="ID" />
			<generator class="identity" />
		</id>

		<property name="ecomId" type="java.lang.Long">
			<column name="ECOM_ID" />
		</property>
		<property name="startTime" type="java.util.Date">
			<column name="START_TIME" />
		</property>
		<property name="endTime" type="java.util.Date">
			<column name="END_TIME" />
		</property>
		<property name="consumeAmount" type="java.lang.Double">
			<column name="CONSUME_AMOUNT" />
		</property>
		<property name="profitAmount" type="java.lang.Double">
			<column name="PROFIT_AMOUNT" />
		</property>
		<property name="status" type="java.lang.Byte">
			<column name="STATUS" />
		</property>

		<property name="createUserId" type="java.lang.Long">
			<column name="CREATE_USER_ID" />
		</property>
		<property name="createDateTime" type="java.util.Date">
			<column name="CREATE_DATE_TIME" />
		</property>
		<property name="updateUserId" type="java.lang.Long">
			<column name="UPDATE_USER_ID" />
		</property>
		<property name="updateDateTime" type="java.util.Date">
			<column name="UPDATE_DATE_TIME" />
		</property>
		<property name="deleted" type="java.lang.String">
			<column name="DELETED" />
		</property>
	</class>
</hibernate-mapping>
