<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.dt.tarmag.model.CustomerRoleUnit" table="DT_CUSTOMER_ROLE_UNIT">
		<cache usage="read-write" />
		<id name="id" type="java.lang.Long">
			<column name="ID" />
			<generator class="identity" />
		</id>

		<property name="unitId" type="java.lang.Long">
			<column name="UNIT_ID" />
		</property>
		<property name="customerId" type="java.lang.Long">
			<column name="CUSTOMER_ID" />
		</property>
		<property name="roleId" type="java.lang.Long">
			<column name="ROLE_ID" />
		</property>

		<property name="deleted" type="java.lang.String">
			<column name="DELETED" />
		</property>
	</class>
</hibernate-mapping>
